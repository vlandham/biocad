<% content_for :head do %>
  <script type="text/javascript" src="/javascripts/swfobject.js"></script>
  <script type="text/javascript">
  	var flashvars = {};
  	flashvars.controller = "genes";
    flashvars.gene_ids = <%= @genes.inspect %>;
  	var params = {};
  	var attributes = {};
  	swfobject.embedSWF("/flash/gene_network.swf", "flash-gene-network", "800", "800", "9.0.0", false, flashvars, params, attributes);
  </script>
<% end %>
