<% content_for :head do %>
  <script type="text/javascript" src="/javascripts/swfobject.js"></script>
  <script type="text/javascript">
  	var flashvars = {};
  	flashvars.controller = "gene_groups";
    flashvars.value_id = <%= @gene_group.id %>;
  	var params = {};
  	var attributes = {};
  	swfobject.embedSWF("/flash/integrated_network.swf", "flash-gene-group-visualization", "800", "800", "9.0.0", false, flashvars, params, attributes);
  </script>
<% end %>
<div class="clearbox">
  
</div>
<div id="flash-gene-group-visualization">
	<a href="http://www.adobe.com/go/getflashplayer">
		<img src="http://www.adobe.com/images/shared/download_buttons/get_flash_player.gif" alt="Get Adobe Flash player" />
	</a>
</div>