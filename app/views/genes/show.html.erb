<h2 class="title"><%= @gene.gene_symbol %></h2>
<div class="info">
  <h2 id="general-info">General Information</h2>

    <h3>Swissprot:</h3>
    <p><%= @gene.swissprot  || 'unknown'%></p>
  
    <h3>Genebank:</h3>
    <p><%= @gene.genebank || 'unknown' %></p>

    <h3>Omim:</h3>
    <p><%= @gene.omim  || 'unknown'%></p>
  
    <h3>Entrez:</h3>
    <p><%= @gene.entrez  || 'unknown' %></p>
  
    <h3>Orf:</h3>
    <p><%= @gene.orf  || 'unknown' %></p>
    
  <h2>Papers</h2>
  <table>
    <tr>
      <th>Title</th><th>Journal</th><th>issue</th><th>year</th><th>pages</th>
    </tr>
    <% 5.times do |num| %>
    <tr>
      <td>Gene paper title <%= num+1 %></td><td>Nature</td><td>11</td><td>200<%= num+3 %></td><td>112-123</td>
    </tr>
    <tr>
      <td>...The consequences to GENX are clearly shown by the results of the... </td>
    </tr>
    <% end %>
  </table>
  <h2>Related Cancers:</h2>
  <h3>Onco</h3>
  <% unless @gene.onco_cancers.empty? %>
    <div class="center">
      <%= render :partial => 'cancers/cancer', :collection => @gene.onco_cancers %>
    </div>
  <% else %>
    <p class="quiet">none</p>
  <% end %>
  
  <div class="clearbox"></div>
  
  <h3>Suppressed</h3>
  
  <% unless @gene.suppressed_cancers.empty? %>
    <div class="center">
      <%= render :partial => 'cancers/cancer', :collection => @gene.suppressed_cancers %>
    </div>
   <% else %>
    <p class="quiet">none</p>
  <% end %>

  
  <div class="clearbox">  
  </div>
  <h2>Interactions:</h2>
  <% unless @gene.gene_interactions.empty? %>
  <div class="center">
    <%= render :partial => 'genes/gene_table', :locals => {:genes => @gene.gene_interactions} %>
  </div>
  <% else %>
    <p class="quiet">none</p>
  <% end %>
  <div class="clearbox">
    
  </div>
  
  <h2>Transcription Factors:</h2>
  <% unless @gene.gene_transcription_factors.empty? %>
    <div class="center">
      <%= render :partial => 'genes/gene_table', :locals => {:genes => @gene.gene_transcription_factors } %>
    </div>
  <% else %>
    <p class='quiet'>none</p>
  <% end %>
  <div class="clearbox">
  </div>
</div>
<%= link_to 'All Genes', genes_path %>   <%= link_to 'Edit', edit_gene_path(@gene) if logged_in? %>


