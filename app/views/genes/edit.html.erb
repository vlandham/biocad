<h1>Editing gene</h1>

<% form_for(@gene) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :gene_symbol %><br />
    <%= f.text_field :gene_symbol %>
  </p>
  <p>
    <%= f.label :swissprot %><br />
    <%= f.text_field :swissprot %>
  </p>
  <p>
    <%= f.label :genebank %><br />
    <%= f.text_field :genebank %>
  </p>
  <p>
    <%= f.label :omim %><br />
    <%= f.text_field :omim %>
  </p>
  <p>
    <%= f.label :entrez %><br />
    <%= f.text_field :entrez %>
  </p>
  <p>
    <%= f.label :orf %><br />
    <%= f.text_field :orf %>
  </p>
  <p>
    <%= f.label :synonym_id %><br />
    <%= f.text_field :synonym_id %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @gene %> |
<%= link_to 'Back', genes_path %>
