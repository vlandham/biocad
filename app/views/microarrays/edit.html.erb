<h1>Editing microarray</h1>

<% form_for(@microarray) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :experiment_id %><br />
    <%= f.text_field :experiment_id %>
  </p>
  <p>
    <%= f.label :filename %><br />
    <%= f.text_field :filename %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @microarray %> |
<%= link_to 'Back', microarrays_path %>
