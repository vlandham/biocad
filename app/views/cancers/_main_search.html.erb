<div class="green left rounded corner big-box">
    <% form_tag cancers_path, :method => 'get', :class => "main-search" do %>
      <div class="label">
        Search by <strong>Cancer</strong>
      </div>
        <%= text_field_tag :search, params[:search], :class => "text" %>
  
        <%= image_submit_tag "Search", :name => nil, :src => "/images/icons/search.png" , :alt => 'search'%>
    <%- end -%>      
</div>
