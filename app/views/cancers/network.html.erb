<% content_for :head do %>
  <script type="text/javascript" src="/javascripts/swfobject.js"></script>
  <script type="text/javascript">
  	var flashvars = {};
  	flashvars.controller = "cancers";
    flashvars.value_id = <%= @cancer.id %>;
  	var params = {};
  	var attributes = {};
  	swfobject.embedSWF("/flash/integrated_network.swf", "flash-integrated-network", "800", "600", "9.0.0", false, flashvars, params, attributes);
  </script>
<% end %>

<div id="flash-integrated-network">
	<a href="http://www.adobe.com/go/getflashplayer">
		<img src="http://www.adobe.com/images/shared/download_buttons/get_flash_player.gif" alt="Get Adobe Flash player" />
	</a>
</div>

